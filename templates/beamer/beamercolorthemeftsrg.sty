
\mode<presentation>

\definecolor{ftsrg@plum}{RGB}{118,37,54}
\definecolor{ftsrg@lightplum}{RGB}{163,51,75}
\definecolor{ftsrg@lighterplum}{RGB}{184,58,85}
\definecolor{ftsrg@examplegreen}{RGB}{85,183,58}

\setbeamercolor{structure}{fg=ftsrg@plum}

\setbeamercolor{palette secondary}{use=structure,fg=ftsrg@lightplum}

\setbeamercolor{titlelike}{bg=structure.fg,fg=white}
\setbeamercolor{demo frametitle}{bg=white,fg=black}
\setbeamercolor{demo frametitle separator}{parent=demo frametitle}

\setbeamercolor{headline}{bg=structure.fg,fg=white}
\setbeamercolor{footline}{bg=structure.fg,fg=white}

\setbeamercolor{block title}{bg=ftsrg@lighterplum,fg=white}
\setbeamercolor{block title alerted}{parent=block title,bg=alerted text.fg}
\setbeamercolor{block title example}{parent=block title,bg=ftsrg@examplegreen}
\setbeamercolor{block body}{bg=black!10}
